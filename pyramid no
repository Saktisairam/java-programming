import java.util.Scanner;
class Main {
    public static void main(String[] args) {
       
        Scanner ss=new Scanner(System.in);
       
        System.out.println("ENTER THE R VALUE:");
        int R=ss.nextInt();
        {
            for(int i=1;i<=R;i++)
            {
                for(int j=1;j<=i;j++)
                System.out.print(j+"");
           
            System.out.println();
            }
        }
    }
}
